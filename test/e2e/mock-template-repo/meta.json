{
    "schema": {
        "name": {
            "type": "string",
            "required": true,
            "label": "Project name"
        },
        "description": {
            "type": "string",
            "required": true,
            "label": "Project description",
            "default": "A San project"
        },
        "author": {
            "type": "string",
            "label": "Author"
        },
        "private": {
            "type": "boolean",
            "default": true
        }
    },
    "skipInterpolation": "src/*-{one,two}.san"
}
